@model _2001215731_LeBuiThienDuc.Models.Deparment

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>showlstEml</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />
</head>
<body>
    <div>
        <h4>Bảng NHân viên</h4>
        <p>Phòng Ban:@Html.DisplayFor(model=>model.Name)</p>
        <table class="container">
            <tr>
                <th>Mã Nhân Viên</th>
                <th>Tên Nhân Viên</th>
                <th>Giới Tính</th>
                <th>Thành Phố</th>
            </tr>
            @foreach (var item in ViewBag.eml)
            {
                <tr>
                    <td>@item.Id</td>
                
                
                    <td>@item.Name</td>
                
                    <td>@item.Gender</td>
                
                    <td>@item.City</td>
                </tr>
            }
        </table>
       </div>
    <p>
  
        @Html.ActionLink("Back to List", "showDDl")
    </p>
</body>
</html>
